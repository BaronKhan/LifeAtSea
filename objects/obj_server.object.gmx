    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Send player data to attached client
if (room == room_coop_test) {
// Once a frame, we send attached client ALL game sprites (plaeyrs and baddies)
var count = ds_list_size(socketlist);
if( count&gt;0 )
{
    /// Send all sprites to all clients
    global.player_buffer = player_buffer;
    
    // Reset buffer to start - Networking ALWAYS reads from the START of the buffer
    buffer_seek(player_buffer, buffer_seek_start, 0);

    // Total number of sprites (players+baddies)
    buffer_write(player_buffer, buffer_u32, 4+instance_number(obj_bullet)+instance_number(par_enemy)
                  +instance_number(obj_bullet_enemy)
                  +instance_number(obj_explode)
                  +instance_number(obj_explode64)
                  +instance_number(obj_explode_256)
                  +instance_number(obj_explode_512)
                  +instance_number(obj_star)
                  +instance_number(obj_star_sparkle)
                  +instance_number(obj_bubble)
                  +instance_number(obj_bubble_pop));  //UPDATE THIS
    
    // Dummy player x,y...will fill in later. This allows the client to follow themselves in a scrolling level.
    buffer_write(global.player_buffer, buffer_s16, 0 );
    buffer_write(global.player_buffer, buffer_s16, 0 );
    
    //Write coop ship oxygen later
    buffer_write(global.player_buffer, buffer_s16, 0 );
    buffer_write(global.player_buffer, buffer_s16, 0 );
    
    buffer_write(global.player_buffer, buffer_s16, score );
    
    buffer_write(global.player_buffer, buffer_s16, obj_ship.dead &amp;&amp; obj_ship.x &lt; -32);
    buffer_write(global.player_buffer, buffer_s16, obj_ship_coop.dead &amp;&amp; obj_ship_coop.x &lt; -32);
    
    buffer_write(global.player_buffer, buffer_s16, obj_controller.decrease_alpha);
    
    var list = ds_list_create();
    //List in order of depth (draw first)
    ds_list_add(list, obj_ship, obj_ship_coop, par_enemy, obj_bullet, obj_bullet_enemy, obj_bubble, obj_explode64, obj_explode,
                  obj_explode_256, obj_explode_512, obj_bubble_pop, obj_star, obj_star_sparkle, obj_cursor, obj_cursor_coop);
    for (var i = 0; i &lt; ds_list_size(list) ; i+=1) {
      var inst = ds_list_find_value(list, i);
      with(inst)
      {
          buffer_write(global.player_buffer, buffer_s16, id );
          buffer_write(global.player_buffer, buffer_s16, object_index );
          buffer_write(global.player_buffer, buffer_s16, x );
          buffer_write(global.player_buffer, buffer_s16, y );
          buffer_write(global.player_buffer, buffer_s16, sprite_index );
          buffer_write(global.player_buffer, buffer_s16, image_index );
          buffer_write(global.player_buffer, buffer_s16, image_blend );
          buffer_write(global.player_buffer, buffer_s16, image_angle );
          buffer_write(global.player_buffer, buffer_s16, image_alpha );
          buffer_write(global.player_buffer, buffer_s16, image_xscale );
          buffer_write(global.player_buffer, buffer_s16, image_yscale );
          if (variable_instance_exists(self, "hp") &amp;&amp; variable_instance_exists(self, "max_hp")) {
            buffer_write(global.player_buffer, buffer_u16, floor(hp) );
            buffer_write(global.player_buffer, buffer_u16, floor(max_hp) );
          } else {
            buffer_write(global.player_buffer, buffer_u16, 0 );
            buffer_write(global.player_buffer, buffer_u16, 0 );
          }
          buffer_write(global.player_buffer, buffer_s16, sprite_width );
          buffer_write(global.player_buffer, buffer_s16, sprite_height );
          if (variable_instance_exists(self, "face_dir")) {
            buffer_write(global.player_buffer, buffer_s16, face_dir );
          } else {
            buffer_write(global.player_buffer, buffer_s16, 0 );
          }
          if (variable_instance_exists(self, "state")) {
            buffer_write(global.player_buffer, buffer_s16, state );
          } else {
            buffer_write(global.player_buffer, buffer_s16, 0 );
          }
      }
    }
    
    var buffer_size = buffer_tell(player_buffer);
        
    // Now send all data... to all clients
    for(i=0;i&lt;count;i++)
    {   
        // get the socket
        var sock = ds_list_find_value(socketlist,i);

        // Write in client location, so it can do a scroll window "follow"
        buffer_seek(player_buffer, buffer_seek_start, 4);

        // Get the player's instance, so we can get it's X,Y
        buffer_write(global.player_buffer, buffer_s16, obj_ship_coop.x );
        buffer_write(global.player_buffer, buffer_s16, obj_ship_coop.y );
      
        // Get the coop chip hp
        buffer_write(global.player_buffer, buffer_s16, obj_ship_coop.oxygen );
        buffer_write(global.player_buffer, buffer_s16, obj_ship_coop.max_oxygen );
        
        // Send data to client
        network_send_packet( sock,player_buffer, buffer_size );
    }
}


}


</string>
