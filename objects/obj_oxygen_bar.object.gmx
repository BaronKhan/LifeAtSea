  var oxygen_colour;
  if ((obj_ship.oxygen*100)/obj_ship.max_oxygen &gt; 20) {
    draw_set_alpha(0.2);
    oxygen_colour = c_blue;
  }
  else {
    draw_set_alpha(0.4);
    oxygen_colour = c_red;
  }
  
  hp -= sign( hp - obj_ship.oxygen ) * min( abs( hp - obj_ship.oxygen ), 1 )
  
  scr_draw_circle(display_get_gui_width()/2,display_get_gui_height()/10,
    display_get_gui_height()/20/*25*/,0,90,360,obj_ship.max_oxygen,oxygen_colour,hp,obj_ship.max_oxygen);
  draw_set_alpha(1);
}
else {
  hp = 200;
}
