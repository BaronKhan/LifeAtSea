            <string>///FSM
dash = false;
switch (state)
{
  case KNIFE_IDLE:
    if (y &lt; 496) {
      gravity = 0.04;
    }
    else {
      hspeed = 0;
      vspeed = 0;
    }
    detected = (instance_number(obj_ship) &gt; 0 &amp;&amp; distance_to_object(obj_ship) &lt; 400);
    if (detected) {
      alarm[4] = room_speed/2;
      state = KNIFE_READY;
    }
    image_angle = (image_angle+(sin(degtorad(image_angle-270))*3));;
    break;
  case KNIFE_READY:
    if (y &lt; 496) {
      gravity = 0.04;
    }
    else {
      hspeed = 0;
      vspeed = 0;
    }
    image_angle = (image_angle+(sin(degtorad(image_angle-270))*3));;
    break;
  case KNIFE_SPIN:
    dash = true;
    if (y &gt; 496) {
      gravity = 0;
      if (alarm[3] &gt; 0 &amp;&amp; instance_number(obj_ship) &gt; 0) {
        move_towards_point(obj_ship.x, obj_ship.y,min(3, alarm[3]/100));
      }
      chasing = (instance_number(obj_ship) &gt; 0 &amp;&amp; distance_to_object(obj_ship) &lt; 300);
      if (!chasing) {
        state = KNIFE_IDLE;
        alarm[3] = -1;
      }
    }
    else {
      gravity = 0.04;
    }

    image_angle = (image_angle + min(20, alarm[3]/10));
    if (image_angle &gt; 360) {
      image_angle = image_angle - 360;
    }
    break;
}
</string>
