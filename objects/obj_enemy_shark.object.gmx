        <arguments>
          <argument>
            <kind>1</kind>
            <string>///FSM
bite = false;
switch (state)
{
  case SHARK_IDLE:
    if (y &lt; 496) {
      gravity = 0.04;
    }
    else {
      hspeed = 0;
      vspeed = 0;
    }
    bite = (instance_number(obj_ship &gt; 0)
            &amp;&amp; distance_to_object(obj_ship) &lt; 64
            &amp;&amp; ((obj_ship.x &lt; x) ^ (face_dir==1))
            );
    if (bite) {
        sprite_index = spr_Shark_64_Idle_Attack;
        image_speed = 0.5;
    }
    else {
      sprite_index = spr_Shark_64_Idle;
      image_speed = 0.125;
    }
    image_angle = (image_angle+(sin(degtorad(image_angle-180))*3));;
    break;
  case SHARK_SWIM:
    vspeed = 0;
    hspeed = face_dir;
    sprite_index = spr_Shark_64_Swim;
    image_speed = 10/room_speed;
    detected = (instance_number(obj_ship) &gt; 0
                &amp;&amp; distance_to_object(obj_ship) &lt; 300
                &amp;&amp; abs(y - obj_ship.y) &lt; 200
                &amp;&amp; ((obj_ship.x &lt; x) ^ (face_dir==1))
                );
    if (detected) {
      alarm[3] = -1;
      alarm[4] = room_speed/2;
      state = SHARK_IDLE;
    }
    //image_angle = (image_angle+(sin(degtorad(image_angle-180))*3));
    break;
  case SHARK_ATTACK:
    if (y &gt; 496) {
      gravity = 0;
      move_towards_point(obj_ship.x, obj_ship.y, 3);
      chasing = (instance_number(obj_ship) &gt; 0
                &amp;&amp; distance_to_object(obj_ship) &lt; 300
                );
      if (!chasing) {
        state = SHARK_IDLE;
        alarm[3] = random_range(room_speed, room_speed*3);
        if (obj_ship.x &lt; x) {
          //Hacky fix
          direction = 0;
          image_angle = 0;
          image_xscale = -1;
          image_yscale = 1;
          face_dir = -1;
          break;
        }
      }
    }
    else {
      gravity = 0.04;
    }
    if ((direction % 360) &lt; 90 || (direction % 360) &gt; 270) { face_dir = 1; }
    else { face_dir = -1; }
    image_angle = direction;
    sprite_index = spr_Shark_64_Swim_Attack;
    image_speed = 0.5;
    break;
  case SHARK_DIE:
    direction = image_angle;
    if ((direction % 360) &lt; 90 || (direction % 360) &gt; 270) { face_dir = 1; }
    else { face_dir = -1; }
    break;
  case SHARK_DEAD:
    vspeed = min(vspeed, 15);
    /*if ((direction % 360) &lt; 90 || (direction % 360) &gt; 270) {
      face_dir = 1;
    }
    else {
      face_dir = -1;
    }*/
    //image_angle = (image_angle+(sin(degtorad(image_angle-direction-180))*3));
    image_speed = 0;
    image_index = image_number -1;
    break;
}
</string>
